<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="product_template_tonnage_ids_form_inherit" model="ir.ui.view">
        <field name="name">product.template.tonnage.ids.form.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view" />
        <field name="arch" type="xml">
            <field name="currency_id" position="after">
                <field name="tonnage_ids" widget="many2many_tags"/>
                <field name="template_quotation" string="Plantilla Asociada" />
            </field>
        </field>
    </record>


</odoo>
